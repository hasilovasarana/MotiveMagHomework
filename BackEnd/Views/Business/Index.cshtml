        </div>
    </div>
    <!--//end-banner-->
    <!--/start-main-->
    <div class="main-content">
        <div class="container">
            <div class="mag-inner">
                <div class="col-md-8 mag-innert-left">
                    <!--/start-Technology-->
                    <div class="technology">
                        <h2 class="tittle"><i class="glyphicon glyphicon-certificate"> </i>@ViewBag.catBusinessName</h2>

                        <div class="col-md-6 tech-img">

                            <img src="~/Upload/@ViewBag.catBusiness[0].news_photo" class="img-responsive" alt="" />

                        </div>
                        <div class="col-md-6 tech-text">
                            @for (int i = 1; i < ViewBag.catBusiness.Count; i++)
                            {
                                <div class="editor-pics">
                                    <div class="col-md-3 item-pic">
                                        <img src="~/Upload/@ViewBag.catBusiness[i].news_photo" class="img-responsive" alt="" />

                                    </div>

                                    <div class="col-md-9 item-details">

                                        <h5 class="inner two"><a href="/Home/Details/@ViewBag.catBusiness[i].id">@ViewBag.catBusiness[i].news_title</a></h5>




                                        <div class="td-post-date two">@ViewBag.catBusiness[i].news_date</div>


                                    </div>


                                    <div class="clearfix"></div>
                                </div>
                            }

                            <div class="clearfix"></div>
                        </div>

                    </div>
                </div>

            </div>
