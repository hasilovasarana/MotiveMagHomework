                        <a class="navbar-brand" href="@Url.Action("Index","Home")"><h1>Motive<span> Mag</span></h1> </a>
                    </div>
                    <!--/.navbar-header-->

                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                        <ul class="nav navbar-nav">
                            <li><a href=@Url.Action("Index","Health")>Health</a></li>
                            <li><a href=@Url.Action("Index","Sports")>Sports</a></li>
                            <li>
                                <a class="active" href=@Url.Action("Index","Entertainment")>Entertainment</a>
                            </li>
                            <li><a href=@Url.Action("Index","Weather")>Weather</a></li>
                            <li>
                                <a href=@Url.Action("Index","Business")>Business</a>
                            </li>
                        </ul>
                    </div>
                    <!--/.navbar-collapse-->
                </div>
            </nav>
            <!--/.navbar-->
        </div>
    </div>
    <!--/start-banner-->
